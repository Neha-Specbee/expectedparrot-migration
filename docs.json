{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "almond",
  "name": "Mint Starter Kit",
  "colors": {
    "primary": "#2563eb",
    "light": "#22c55e",
    "dark": "#2563eb"
  },
  "favicon": "/favicon.png",
  "appearance": {
    "default": "light"
  },
  "background": {
    "color": {
      "dark": "#23272f"
    }
  },
  "navigation": {
    "versions": [
      {
        "version": "latest",
        "tabs": [
          {
            "tab": "Introduction",
            "pages": [
              "en/latest",
              "en/latest/overview",
              "en/latest/whitepaper",
              "en/latest/papers"
            ]
          },
          {
            "tab": "Getting Started",
            "pages": [
              "en/latest/getting_started",
              "en/latest/installation",
              "en/latest/api_key"
            ]
          },
          {
            "tab": "API reference",
            "groups": [
              {
                "group": "API documentation",
                "pages": []
              }
            ]
          }
        ]
      }
    ],
    "global": {
      "anchors": [
        {
          "anchor": "Community",
          "href": "https://discord.com/invite/mxAYkjfy9m",
          "icon": "discord"
        },
        {
          "anchor": "Blog",
          "href": "https://blog.expectedparrot.com/",
          "icon": "newspaper"
        }
      ]
    }
  },
  "logo": {
    "light": "/logo/light.svg",
    "dark": "/logo/dark.svg",
    "href": "https://expectedparrot.com"
  },
  "navbar": {
    "links": [
      {
        "label": "Get Started",
        "href": "https://expectedparrot.com/getting-started"
      }
    ],
    "primary": {
      "type": "button",
      "label": "Login in / Sign up",
      "href": "https://expectedparrot.com/login"
    }
  },
  "contextual": {
    "options": [
      "copy",
      "view",
      "chatgpt",
      "claude",
      "perplexity",
      "mcp",
      "cursor",
      "vscode"
    ]
  },
  "footer": {
    "socials": {
      "github": "https://github.com/expectedparrot/edsl"
    }
  }
}